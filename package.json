{
  "name": "typescript-boilerplate",
  "version": "1.0.0",
  "description": "TypeScript, SCSS実装用",
  "main": "index.ts",
  "scripts": {
    "typecheck": "tsc -w",
    "webpack-dev": "npx webpack --evn.file=development --config webpack.dev.js",
    "webpack-server": "npx webpack-dev-server --env.file=development --config webpack.dev.js",
    "dev": "npm run typecheck & npm run webpack-dev & npm run webpack-server",
    "build:development": "npx webpack --env.file=development --config webpack.prod.js",
    "build:staging": "npx webpack --env.file=staging --config webpack.prod.js",
    "build:production": "npx webpack --env.file=production --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astatsuya/TypeScript-boilerplate.git"
  },
  "author": "astatsuya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/astatsuya/TypeScript-boilerplate/issues"
  },
  "homepage": "https://github.com/astatsuya/TypeScript-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "browserlist": "^1.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^3.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
